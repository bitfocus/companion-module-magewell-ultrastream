{
	"name": "magewell-ultrastream",
	"version": "1.0.1",
	"main": "dist/index.js",
	"type": "module",
	"scripts": {
		"postinstall": "husky",
		"format": "prettier -w .",
		"package": "run build && companion-module-build",
		"build": "rimraf dist && run build:main",
		"build:main": "tsc -p tsconfig.build.json",
		"dev": "tsc -p tsconfig.build.json --watch",
		"lint:raw": "eslint",
		"lint": "run lint:raw ."
	},
	"author": "Aart Jan Kaptijn <aartjan@kaptijn.online>",
	"license": "MIT",
	"dependencies": {
		"@companion-module/base": "~1.11.3",
		"axios": "^1.13.5"
	},
	"devDependencies": {
		"@companion-module/tools": "^2.6.1",
		"@types/node": "^22.15.32",
		"eslint": "^9.29.0",
		"husky": "^9.1.7",
		"lint-staged": "^15.5.2",
		"prettier": "^3.5.3",
		"rimraf": "^6.0.1",
		"typescript": "~5.8.3",
		"typescript-eslint": "^8.34.1"
	},
	"homepage": "https://github.com/bitfocus/companion-module-magewell-ultrastream#readme",
	"bugs": {
		"url": "https://github.com/bitfocus/companion-module-magewell-ultrastream/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bitfocus/companion-module-magewell-ultrastream.git"
	},
	"api_version": "1.0.0",
	"prettier": "@companion-module/tools/.prettierrc.json",
	"lint-staged": {
		"*.{css,json,md,scss}": [
			"prettier --write"
		],
		"*.{ts,tsx,js,jsx}": [
			"yarn lint:raw --fix"
		]
	},
	"engines": {
		"node": ">=22.0.0"
	},
	"packageManager": "yarn@4.12.0"
}
